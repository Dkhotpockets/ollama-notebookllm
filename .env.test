# Test Environment Configuration for NotebookLlama

# OpenAI API (required for LLM functionality)
OPENAI_API_KEY=sk-test-key-placeholder

# LlamaCloud API (required for indexing)
LLAMACLOUD_API_KEY=llx-test-key-placeholder

# ElevenLabs API (for podcast generation)
ELEVENLABS_API_KEY=sk_test-key-placeholder

# PostgreSQL Configuration (optional for testing)
pgql_db=test_db
pgql_user=test_user
pgql_psw=test_password

# Supabase Configuration (mock for testing)
SUPABASE_URL=http://localhost:54321
SUPABASE_KEY=test-supabase-key

# Neo4j Configuration (optional for testing)
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=test-password

# Google API (optional)
GOOGLE_API_KEY=test-google-api-key

# Ollama Configuration (optional)
OLLAMA_HOST=http://localhost:11434

# RAGFlow API Configuration
RAGFLOW_API_KEY=test-ragflow-key

# Feature Toggles (disable for testing to avoid dependencies)
ENABLE_VECTOR_SEARCH=false
ENABLE_KNOWLEDGE_GRAPH=false
ENABLE_WEB_CRAWLING=false
ENABLE_MULTI_LLM=false

# Logging
RAGFLOW_LOG_LEVEL=ERROR
